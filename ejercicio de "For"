#include <stdio.h>

int main(void) {
	
	int notas;
	int i=0;
	int n=10;
	float promedios =0; 
    
	for(i = 0; i < n; i++)
{	
	
	printf ("Ingrese las notas");
	scanf ("%d",&notas);
	
	promedios = promedios+notas;
}
	promedios = promedios/n;
	 
	printf("El promedio es: %f",promedios);
	
	
	return 0;
}
