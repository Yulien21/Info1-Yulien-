#include <stdio.h>

int main(void) {
	
	int num1;
		
	printf("ingrese un numero del 0 al 100: ");
		scanf("%d",&num1);
	
	while(num1 < 0 || num1 > 100){
	printf("El numero esta fuera del rango,vuelva a ingresar\n");
	printf("ingrese un numero del 0 al 100: ");
	scanf("%d",&num1);
	}
	printf("El numero ingresado es:%d\n",num1);
	
	return 0;
